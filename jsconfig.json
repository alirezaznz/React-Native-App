{
    "compilerOptions": {
      "baseUrl": "./",
      "paths": {
        "@Constants": ["./src/constants/*"],
        "@Utils": ["./src/utils/*"],
        "@Services": ["./src/services/*"],
        "@Reducers": ["./src/redux/sliceReducers/*"],
        "@Components": ["./src/components/*"],
        "@Theme": ["./src/theme/*"],

      },
      "checkJs": true,
      "jsx": "react",
      "allowSyntheticDefaultImports": true
    },
    "include": [
      "./src/",
      "./App.jsx"
    ],
    "exclude": ["node_modules"]
  }