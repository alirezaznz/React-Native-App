{
  "compilerOptions": {
    "outDir": "build",
      "baseUrl": ".",
      "paths": {
        "@Constants": ["./src/constants/*"],
        "@Utils": ["./src/utils/*"],
        "@Services": ["./src/services/*"],
        "@Reducers": ["./src/redux/sliceReducers/*"],
        "@Components": ["./src/components/*"],
      },
      "checkJs": true,
      "allowJs": true,
      
      "jsx": "react",
      // "tsx": "react",
      "allowSyntheticDefaultImports": true
      },
    "include": [
        "./src/",
      ],
  "exclude": ["node_modules"]
}