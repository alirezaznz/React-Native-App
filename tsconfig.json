{
  "compilerOptions": {
    "moduleResolution": "node",
    "outDir": "build",
      "baseUrl": ".",
      "paths": {
        "@Constants": ["./src/constants/*"],
        "@Utils": ["./src/utils/*"],
        "@Services": ["./src/services/*"],
        "@Reducers": ["./src/redux/sliceReducers/*"],
        "@Components": ["./src/components/*"],
      },
      "checkJs": false,
      "allowJs": true,
      
      "jsx": "react",
      // "tsx": "react",
      "allowSyntheticDefaultImports": true
      },
    "include": [
        "*",
        // "App.jsx"
      ],
  "exclude": ["node_modules"]
}